{
	"name": "client",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"vite dev --host\" \"bun run tw:watch\" -p none --hide 1",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"tw": "tailwindcss -i ./src/app.css -o ./static/tailwind.css",
		"tw:watch": "tailwindcss -i ./src/app.css -o ./static/tailwind.css --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.16",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/bun": "^1.1.4",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"concurrently": "^8.2.2",
		"daisyui": "^4.12.2",
		"date-picker-svelte": "^2.13.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.5",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.4",
		"svelte": "^5.0.0-next.155",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^3.8.0",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"vite": "^5.3.1"
	},
	"type": "module"
}
